{
  "name": "pedal-build",
  "version": "0.1.0",
  "description": "Intelligent guitar pedal workflow application with AI-powered agents",
  "main": "src/backend/main.py",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "uvicorn src.backend.main:app --reload --port 8000",
    "dev:frontend": "next dev",
    "build": "npm run generate:types && npm run build:frontend",
    "build:frontend": "next build",
    "start": "uvicorn src.backend.main:app --host 0.0.0.0 --port 8000",
    "setup:db": "python scripts/setup-db.py",
    "setup:python": "uv pip install -e '.[dev]'",
    "setup:venv": "uv venv && source .venv/bin/activate",
    "generate:types": "python scripts/generate-types.py",
    "precommit:install": "pre-commit install",
    "validate:types": "python scripts/validate-types.py",
    "test": "pytest",
    "test:py": "pytest tests/",
    "test:coverage": "pytest --cov=src tests/",
    "lint": "next lint && ruff check src/",
    "lint:py": "ruff check src/ && mypy src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" && black src/",
    "format:py": "black src/",
    "type-check": "tsc --noEmit && python scripts/validate-types.py"
  },
  "keywords": [
    "guitar-pedals",
    "electronics",
    "diy",
    "breadboard",
    "schematic-analysis",
    "ai-agents",
    "google-adk",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/adk": "latest",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "lucide-react": "^0.344.0",
    "next": "15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
